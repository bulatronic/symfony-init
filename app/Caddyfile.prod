{
    frankenphp
# Email for Let's Encrypt notifications (optional)
# email example@example.com
}

# Main domain

symfony-init.dev {
    root * /app/public

    encode zstd br gzip

    php_server {
                   try_files {path} index.php
               }

    file_server

    log {
            output stdout
            format console
        }
}

# Redirect from www to main domain (optional)

www.symfony-init.dev {
    redir https://symfony-init.dev {uri} permanent
}
